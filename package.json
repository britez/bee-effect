{
  "name": "bee-effect",
  "version": "1.0.0",
  "description": "Webapp for bee-effect",
  "private": true,
  "author": "redbee",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all --parallel build-server build-client",
    "build-server": "NODE_ENV=production webpack --env.ENVIRONMENT=production --progress --config webpack.server.config.js",
    "build-client": "NODE_ENV=production webpack --env.ENVIRONMENT=production --progress",
    "dev": "npm-run-all --parallel server client",
    "server": "nodemon index.js",
    "client": "webpack --env.ENVIRONMENT=development --watch --progress",
    "bundlesize": "bundlesize",
    "precommit": "eslint src --quiet"
  },
  "dependencies": {
    "auth0-js": "^9.10.0",
    "auth0-lock": "^11.14.1",
    "bootstrap": "^4.1.3",
    "isomorphic-fetch": "^2.2.1",
    "jwt-decode": "^2.2.0",
    "local-storage": "^1.4.2",
    "node-sass": "^4.10.0",
    "react": "^16.6.3",
    "react-d3-radar": "^1.0.0-rc4",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "recharts": "^1.4.1",
    "redux": "^4.0.1",
    "redux-devtools": "^3.5.0",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "style-loader": "^0.23.1"
